{
  "name": "zelda",
  "version": "0.0.0",
  "scripts": {
    "start": "concurrently \"npm run angular:serve\" \"npm run electron:serve\"",
    "angular:serve": "ng serve",
    "electron:serve": "wait-on http-get://localhost:5200 && electron . --serve",
    "build:electron": "electron-rebuild -f -w node-pty",
    "build:tools": "tsc -p ./builder/tsconfig.json",
    "preinstall": "npm i -g --production windows-build-tools",
    "postinstall": "npm run build:tools && npm run build:electron",
    "electron-package": "ng build --prod --base-href ./ && electron-packager . --no-prune --ignore=/e2e --ignore=/src"
  },
  "main": "main.js",
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/cdk": "^8.0.0-rc.2",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/forms": "~8.0.0",
    "@angular/material": "^8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "ngx-scrollbar": "^4.1.1",
    "core-js": "^2.5.4",
    "glob": "^7.1.3",
    "node-pty": "^0.8.1",
    "rxjs": "~6.5.2",
    "strip-json-comments": "^2.0.1",
    "tslib": "^1.9.0",
    "xterm": "^3.12.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.0",
    "@angular/cli": "~8.0.0",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "@types/webpack": "^4.4.26",
    "wait-on": "^3.2.0",
    "tree-kill": "^1.2.1",
    "codelyzer": "~4.5.0",
    "concurrently": "^3.6.1",
    "electron": "4.0.5",
    "electron-rebuild": "^1.8.4",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~3.1.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "~5.4.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "typescript": "~3.4.5",
    "webpack-angular-externals": "^1.0.2"
  }
}
